        <Key android:codes="111" android:keyLabel="ᒫ" android:keyOutputText = "ᒫ"/>
        <Key android:codes="1200" android:keyLabel="ᓈ" android:keyOutputText = "ᓈ"/>
        <Key android:codes="-45" android:keyWidth="12%p"
            android:keyIcon="@drawable/down_arrow"
            android:keyEdgeFlags="right"/>
    </Row>

    <Row>

        <Key android:codes="97"  android:keyLabel="▼"  android:keyEdgeFlags="left"/>
